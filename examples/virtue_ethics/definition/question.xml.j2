                var inspo_index = Math.floor(Math.random() * inspo.length);
                var randInspo = inspo[inspo_index];
                insertMessage(randInspo);
            }

            /* Sanity Check */
            function damerau_levenshtein(source, target) {
                if (!source) return target ? target.length : 0;
                else if (!target) return source.length;

                var m = source.length, n = target.length, INF = m + n, score = new Array(m + 2), sd = {};
                for (var i = 0; i < m + 2; i++) score[i] = new Array(n + 2);
                score[0][0] = INF;
                for (var i = 0; i <= m; i++) {
                    score[i + 1][1] = i;
                    score[i + 1][0] = INF;
                    sd[source[i]] = 0;
                }
                for (var j = 0; j <= n; j++) {
                    score[1][j + 1] = j;
                    score[0][j + 1] = INF;
                    sd[target[j]] = 0;
                }

                for (var i = 1; i <= m; i++) {
                    var DB = 0;
                    for (var j = 1; j <= n; j++) {
                        var i1 = sd[target[j - 1]],
                            j1 = DB;
                        if (source[i - 1] === target[j - 1]) {
                            score[i + 1][j + 1] = score[i][j];
                            DB = j;
                        }
                        else {
                            score[i + 1][j + 1] = Math.min(score[i][j], Math.min(score[i + 1][j], score[i][j + 1])) + 1;
                        }
                        score[i + 1][j + 1] = Math.min(score[i + 1][j + 1], score[i1] ? score[i1][j1] + (i - i1 - 1) + 1 + (j - j1 - 1) : Infinity);
                    }
                    sd[source[i - 1]] = i;
                }
                return score[m + 1][n + 1];
            }
            function getAvoidWords(s1, s2) {
                var proofOfLaziness = '';
                var s1Arr = s1.replace(/[^\w\s]|_/g, '').split(' ');
                var s2Arr = s2.replace(/[^\w\s]|_/g, '').split(' ');
                for (var i = 0; i < s1Arr.length; i++) {
                    if (avoid.has(s1Arr[i])) {
                        proofOfLaziness += s1Arr[i] + ', ';
                    }
                }
                for (var i = 0; i < s2Arr.length; i++) {
                    if (avoid.has(s2Arr[i])) {
                        proofOfLaziness += s2Arr[i] + ', ';
                    }
                }
                return proofOfLaziness;
            }
            function verify() {
                var scenario1 = document.getElementById('scenario1').value;
                var scenario2 = document.getElementById('scenario2').value;
                var similarFloat = damerau_levenshtein(scenario1, scenario2) / (0.5 * (scenario1.length + scenario2.length));
                var trait1 = document.getElementById('trait1').value;
                var trait2 = document.getElementById('trait2').value;
                let message;
                if (similarFloat === 0.0) {
                    message = 'Those scenarios are too similar. Please adjust. See the "Examples" tab of "View Instructions."';
                } else if (similarFloat >= 0.50) {
                    message = 'Those scenarios are too dissimilar. Please adjust. See the "Examples" tab of "View Instructions."';
                } else if (scenario1.length === 0 || scenario2.length === 0) {
                    message = 'Please enter valid scenarios. See the "Examples" tab of "View Instructions."';
                } else if (scenario1.length < 40 || scenario2.length < 40) {
                    message = 'Scenarios need to be more intricate or detailed than that. See the "Examples" tab of "View Instructions."';
                } else if (!trait1 || !trait1.trim() || !trait2 || !trait2.trim()) {
                    message = 'Please enter valid traits. See the "Examples" tab of "View Instructions."';
                } else if (trait1.trim() === trait2.trim()) {
                    message = 'Those traits are too similar. Please adjust.';
                } else {
                    var avoidWords = getAvoidWords(scenario1, scenario2);
                    if (avoidWords) {
                        var newMessageContainer = document.createElement('span');
                        var link = document.createElement('a');
                        link.setAttribute('href', 'https://en.wikipedia.org/wiki/Show,_don\'t_tell');
                        link.appendChild(document.createTextNode('Show, don\'t tell.'));
                        var warning = document.createTextNode(' Avoid giveaway words, including: ' + avoidWords.slice(0, avoidWords.length - 2));
                        newMessageContainer.appendChild(link);
                        newMessageContainer.appendChild(warning);
                        message = newMessageContainer;
                    } else {
                        document.querySelector('crowd-button').disabled = false;
                    }
                }
                if (message) {
                    document.querySelector('crowd-button').disabled = true;
                }
                insertMessage(message);
            }

            var buttons = {
                'Sanity Check': verify,
                'Get Action': getVerbs,
                'Get Character Trait': getTraits,
                'Get Inspiration': getInspo,
            }
            
            function onClick(event) {
                var buttonType = event.srcElement.textContent;
                buttons[buttonType]();
            }
            function addEvent(button) {
                button.addEventListener('click', onClick);
            }
            document.querySelectorAll('.suggestion').forEach(addEvent);
            
            // Traverse DOM for dynamically inserted Submit button to disable
            function disable() {
                document.querySelector('crowd-button').disabled = true;
            }
            setTimeout(disable, 3000);
        </script>
